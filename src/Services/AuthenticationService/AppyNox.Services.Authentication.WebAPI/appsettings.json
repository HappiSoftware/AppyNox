{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConsulConfig": {
    "Address": "{Consul HOST}"
  },
  "Consul": {
    "ServiceId": "{}",
    "ServiceName": "{}",
    "Scheme": "{http or https}",
    "ServiceHost": "{}",
    "ServicePort": { "PORT" },
    "Tags": [ "Tag1", "Tag2" ],
    "HealthCheckUrl": "{health-check-url}",
    "HealthCheckIntervalSeconds": 30,
    "HealthCheckTimeoutSeconds": 5
  }
}