{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DevelopmentConnection": "",
    "StagingConnection": "",
    "ProductionConnection": "",
    "DefaultConnection": "",
    "TestConnection": "" // for integration tests, use this to connect to dockerized database container from localhost
  },
  "ConsulConfig": {
    "Address": "{Consul HOST}"
  },
  "Consul": {
    "ServiceId": "{}",
