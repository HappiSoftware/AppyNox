{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Debug" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Debug",
        "Args": { "restrictedToMinimumLevel": "Debug" }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "",
    "TestConnection": "",
    "SagaConnection": ""
  },
  "JwtSettings": {
    "AppyNox": {
      "SecretKey": "",
      "Issuer": "NoxAuthServer",
      "Audience": "AppyNox",
      "TokenLifetimeMinutes": "5"
    },
    "AppyFleet": {
      "SecretKey": "",
      "Issuer": "NoxAuthServer",
      "Audience": "AppyFleet",
      "TokenLifetimeMinutes": "5"
    }
  },
  "ConsulConfiguration": {
    "Address": "http://localhost:8500"
  },
  "Consul": {
    "ServiceId": "SsoService",
    "ServiceName": "SsoService",
    "Scheme": "http",
    "ServiceHost": "localhost",
    "ServicePort": "7001",
    "Tags": [ "Sso", "SSO" ],
    "HealthCheckUrl": "api/health",
    "HealthCheckIntervalSeconds": 30,
    "HealthCheckTimeoutSeconds": 5
  },
  "MessageBroker": {
    "Host": "rabbitmq://localhost:5672",
    "Username": "guest",
    "Password": "guest"
  },
  "NoxMessageBroker": {
    "Host": "rabbitmq://localhost:5673",
    "Username": "guest",
    "Password": "guest"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  }
}